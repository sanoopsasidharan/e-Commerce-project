    <script>
      


        // $("#txtstartdate").datepicker({
        //     minDate: 0,
        //     onSelect: function (date) {
        //         $("#txtenddate").datepicker('option', 'minDate', date);

        //     }
        // });

        // $("#txtenddate").datepicker({});



        $('#add-new-coupon').submit((num) => {
            num.preventDefault()
            $.ajax({
                url: '/admin/add-new-coupon',
                data: $('#add-new-coupon').serialize(),
                method: 'post',
                success: (response) => {
                    if (response.status) {
                        location.reload()
                    }
                }
            })
        })
        function deleteCoupon(couponId) {
            Swal.fire({
                title: 'Are you sure?',
                text: "You won't delete this Coupon!",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: ' delete '
            }).then((result) => {
                if (result.isConfirmed) {
                    $.ajax({
                        url: "/admin/delete-coupon",
                        data: { couponId: couponId },
                        method: 'post',
                        success: (response) => {
                            if (response.status) {
                                Swal.fire(
                                    'Deleted!',
                                    'Coupon has been deleted.',
                                    'success'
                                )
                                location.reload()
                            } else {
                                alert('somthing error plz delete again')
                            }
                        }
                    })

                }
            })
        }
    </script>

