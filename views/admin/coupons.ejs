<body data-topbar="dark">

    <!-- <body data-layout="horizontal"> -->

    <!-- Begin page -->
    <div id="layout-wrapper">




        <!-- ========== Left Sidebar Start ========== -->

        <!-- Left Sidebar End -->

        <!-- Modal -->
        <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <h5 class="modal-title" id="exampleModalLabel">+ coupon</h5>
                        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                    </div>
                    <div class="modal-body">
                        <form id="add-new-coupon">
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="mb-3">
                                        <label for="basicpill-firstname-input" class="form-label">Coupon Name</label>
                                        <input onkeyup="namevalidation()" type="text" name="couponName"
                                            class="form-control" id="basicpill-firstname-input">
                                        <div class="invalid-feedback" id="name-Text">enter your Name</div>
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="mb-3">
                                        <label for="basicpill-lastname-input" class="form-label">Coupon Expiry
                                            date</label>

                                        <input name="couponExDate" type="date" class="form-control" id="date1">
                                      
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="mb-3">
                                        <label for="basicpill-phoneno-input" class="form-label">Coupon
                                            percentage</label>
                                        <input type="text" name="percentage" class="form-control"
                                            id="basicpill-phoneno-input">
                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="mb-3">
                                        <label for="basicpill-phoneno-input" class="form-label">Coupon quantity</label>
                                        <input type="text" name="couponQuantity" class="form-control"
                                            id="coupon-quantity">
                                    </div>
                                </div>

                            </div>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="mb-3">
                                        <label for="basicpill-phoneno-input" class="form-label">minimum price</label>
                                        <input type="text" name="minimumPrice" class="form-control" id="minimum-price">
                                    </div>
                                </div>



                            </div>
                            <div style="display: flex; justify-content: right;">
                                <button type="submit" class="btn btn-dark waves-effect waves-light">submit</button>
                            </div>
                        </form>
                    </div>

                </div>
            </div>
        </div>



        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->

                    <!-- end page title -->


                    <!-- end row -->

                    <div class="row">
                        <div class="col-lg-12">
                            <div class="card">
                                <div style="display: flex; justify-content: space-between;" class="card-header">
                                    <h4 class="card-title mb-0">show all Coupons</h4>
                                    <button type="button" class="btn btn-primary" data-bs-toggle="modal"
                                        data-bs-target="#exampleModal">
                                        + Coupon
                                    </button>
                                </div>
                                <div class="card-body">


                                    <div id="" class="table-responsive">
                                        <% if(coupons.length> 0){ %>
                                            <table class="table">
                                                <thead>
                                                    <tr>
                                                        <th scope="col">Coupon Name</th>
                                                        <th scope="col">Coupon Expiry date</th>
                                                        <th scope="col">Coupon Percentage</th>
                                                        <th scope="col">Minimum price</th>
                                                        <th scope="col">code </th>
                                                        <th scope="col">Coupon quantity</th>
                                                        <th scope="col"> action</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% for(var i=0; i<coupons.length; i++) {%>
                                                        <tr>
                                                            <td>
                                                                <%= coupons[i].couponName %>
                                                            </td>
                                                            <td>
                                                                <%= coupons[i].couponExDate %>
                                                            </td>
                                                            <td>
                                                                <%= coupons[i].percentage %>
                                                            </td>
                                                            <td>
                                                                <%= coupons[i].code %>
                                                            </td>
                                                            <td>
                                                                <%= coupons[i].minimumPrice %>
                                                            </td>
                                                            <td>
                                                                <%= coupons[i].couponQuantity %>
                                                            </td>
                                                            <td>
                                                                <div>

                                                                    <button type="button" class="btn btn-outline-danger"
                                                                        onclick="deleteCoupon('<%= coupons[i]._id %>')">Delete</button>
                                                                </div>
                                                            </td>
                                                        </tr>
                                                        <% } %>
                                                </tbody>

                                            </table>
                                            <% } else{ %>
                                                <div style="display: flex; justify-content: center;">

                                                    <h2 class="text-danger mt-5">No Coupons</h2>
                                                </div>
                                                <% } %>

                                    </div>
                                </div>
                                <!-- end card body -->
                            </div>
                            <!-- end card -->
                        </div>
                        <!-- end col -->
                    </div>
                    <!-- end row -->
                </div> <!-- container-fluid -->
            </div>
            <!-- End Page-content -->
            <!-- Modal -->
            <div class="modal fade confirmModal" tabindex="-1" aria-hidden="true">
                <div class="modal-dialog modal-dialog-centered">
                    <div class="modal-content">
                        <div class="modal-header border-bottom-0">
                            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                        </div>
                        <div class="modal-body">
                            <div class="text-center">
                                <div class="mb-3">
                                    <i class="bx bx-check-circle display-4 text-success"></i>
                                </div>
                                <h5>Confirm Save Changes</h5>
                            </div>
                        </div>
                        <div class="modal-footer justify-content-center">
                            <button type="button" class="btn btn-light w-md" data-bs-dismiss="modal">Close</button>
                            <button type="button" class="btn btn-primary w-md" data-bs-dismiss="modal"
                                onclick="nextTab()">Save changes</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- end modal -->
            <footer class="footer">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-sm-6">
