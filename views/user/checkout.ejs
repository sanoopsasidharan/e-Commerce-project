<body class="checkout_page">
    <div id="ec-overlay"><span class="loader_img"></span></div>

    <!-- Header start  -->
    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                </div>
                <div class="modal-body">
                    <div id="add-address-form" class="ec-check-bill-form">
                        <form action="" id="checkout-form">
                            <span class="ec-bill-wrap ec-bill-half">
                                <label>First Name*</label>
                                <input id="fname" type="text" name="name" placeholder="Enter your Name">
                                <div class="invalid-feedback text-danger mb-3" id="name-Text">enter
                                    your Name</div>
                            </span>

                            <span class="ec-bill-wrap ec-bill-half">
                                <label>Number*</label>
                                <input id="numberValidation" type="text" name="number" placeholder="Enter your Number"
                                    maxlength="10" />
                                <div class="invalid-feedback text-danger mb-3" id="text-Number">
                                    Enter your phone number </div>
                            </span>
                            <span class="ec-bill-wrap">
                                <label>Address</label>
                                <input type="text" id="address" name="address" placeholder="Address Line 1" />
                                <div class="invalid-feedback text-danger mb-3" id="address-Text">
                                    enter your Name</div>
                            </span>
                            <span class="ec-bill-wrap">
                                <label>Location</label>
                                <input id="location" type="text" name="location" placeholder=" current location" />
                                <div class="invalid-feedback text-danger mb-3" id="location-Text">
                                    enter your location</div>
                            </span>
                            <span class="ec-bill-wrap">
                                <label>City</label>
                                <input id="city" type="text" name="City" placeholder=" current city" />
                                <div class="invalid-feedback text-danger mb-3" id="city-Text">enter
                                    your city</div>
                            </span>
                            <span class="ec-bill-wrap ec-bill-half">
                                <label>Post Code</label>
                                <input type="text" id="pinValidation" name="pincode" placeholder="Pin code" />
                                <div class="invalid-feedback text-danger mb-3" id="text-pin">Enter
                                    your pin </div>
                            </span>
                            <span class="ec-bill-wrap">
                                <label>state</label>
                                <input id="state" type="text" name="state" placeholder="state" />
                                <div class="invalid-feedback text-danger mb-3" id="state-Text">Enter
                                    your state </div>
                            </span>
                            <input type="text" name="userId" value="<%=user._id%>" id="" hidden>
                            <button class="btn btn btn-primary Flat-right" type="submit">Submit</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Header End  -->

    <!-- ekka Cart Start -->
    <div class="ec-side-cart-overlay"></div>
    <div id="ec-side-cart" class="ec-side-cart">
        <div class="ec-cart-inner">
            <div class="ec-cart-top">
                <div class="ec-cart-title">
                    <span class="cart_title">My Cart</span>
                    <button class="ec-close">×</button>
                </div>
